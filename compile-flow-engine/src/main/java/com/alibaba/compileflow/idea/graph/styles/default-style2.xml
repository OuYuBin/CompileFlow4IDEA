    </add>
    <add as="choreographyTask" extend="defaultVertex">
        <add as="shape" value="activity"/>
        <add as="type" value="choreography_task"/>
        <add as="shadow" value="0"/>
        <add as="fillColor" value="#99FF99"/>
    </add>
    <add as="choreographySubprocess" extend="defaultVertex">
        <add as="shape" value="subprocess"/>
        <add as="type" value="choreography_subprocess"/>
        <add as="shadow" value="0"/>
        <add as="fillColor" value="#CCCCFF"/>
    </add>
    <add as="choreographyParticipant" extend="defaultVertex">
        <add as="shape" value="participantBand"/>
        <add as="verticalAlign" value="top"/>
        <add as="spacingTop" value="5"/>
        <add as="fillColor" value="#999999"/>
        <add as="border" value="1"/>
    </add>
    <add as="participantTop" extend="choreographyParticipant">
        <add as="position" value="top"/>
    </add>
    <add as="participantBottom" extend="choreographyParticipant">
        <add as="position" value="bottom"/>
    </add>
    <add as="participantAdditionalTop" extend="choreographyParticipant">
