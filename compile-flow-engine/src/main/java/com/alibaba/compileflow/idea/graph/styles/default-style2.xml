        <add as="verticalLabelPosition" value="bottom"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
    </add>
    <add as="pool" extend="defaultVertex">
        <add as="shape" value="swimlane"/>
        <add as="auto" value="1"/>
        <add as="spacingTop" value="5"/>
        <add as="shadow" value="0"/>
        <add as="fontSize" value="14"/>
        <add as="fontStyle" value="1"/>
        <add as="startSize" value="25"/>
        <add as="horizontal" value="0"/>
        <add as="fillColor" value="#999999"/>
        <add as="fontColor" value="#000000"/>
        <add as="foldable" value="true"/>
        <!--<add as="image" value="/com/timout/bpmn/images/marker_multiple.png"/>-->
    </add>
    <add as="lane" extend="pool">
        <add as="opacity" value="30"/>
        <add as="textOpacity" value="100"/>
        <add as="fillColor" value="#FFFF99"/>
        <add as="fontColor" value="#000000"/>
        <add as="foldable" value="false"/>
    </add>
    <add as="verticalPool" extend="pool">
        <add as="horizontal" value="1"/>
    </add>
    <add as="verticalLane" extend="lane">
        <add as="horizontal" value="1"/>
    </add>
    <add as="association" extend="defaultEdge">
        <add as="dashed" value="1"/>
        <add as="edgeStyle" value="none"/>
        <add as="startArrow" value="none"/>
        <add as="endArrow" value="none"/>
    </add>
    <add as="conversationLink" extend="defaultEdge">
        <add as="shape" value="conversationLink"/>
        <add as="sourcePerimeterSpacing" value="3"/>
        <add as="targetPerimeterSpacing" value="3"/>
    </add>
    <add as="messageFlow" extend="defaultEdge">
        <add as="dashed" value="1"/>
        <add as="elbow" value="vertical"/>
        <add as="startArrow" value="open_oval"/>
        <add as="endArrow" value="open_block"/>
    </add>
    <add as="dataAssociation" extend="defaultEdge">
        <add as="dashed" value="1"/>
        <add as="startArrow" value="none"/>
        <add as="endArrow" value="open"/>
    </add>
    <add as="compensationAssociation" extend="dataAssociation">
    </add>
    <add as="message" extend="image">
        <add as="type" value="message"/>
        <add as="labelPosition" value="right"/>
        <add as="verticalLabelPosition" value="middle"/>
        <add as="align" value="left"/>
        <add as="verticalAlign" value="middle"/>
    </add>
    <add as="initiatingMessage" extend="message">
        <add as="init" value="1"/>
        <add as="image" value="/com/timout/bpm/images/message_initiating.png"/>
    </add>
    <add as="nonInitiatingMessage" extend="message">
        <add as="init" value="0"/>
        <add as="image" value="/com/timout/bpm/images/message_non_initiating.png"/>
    </add>
    <add as="activity" extend="defaultVertex">
        <add as="rounded" value="1"/>
    </add>
    <add as="task" extend="activity">
        <add as="shape" value="activity"/>
        <add as="fillColor" value="#f0f5ff"/>
    </add>
    <add as="callTask" extend="task">
        <add as="strokeWidth" value="3"/>
    </add>
    <add as="exclusiveGateway" extend="gateway">
    </add>
    <add as="exclusiveGatewayWithIndicator" extend="exclusiveGateway">
    </add>
    <add as="callProcess" extend="activity">
        <add as="shape" value="subprocess"/>
        <add as="strokeWidth" value="3"/>
        <add as="fillColor" value="#f0f5ff"/>
    </add>
    <add as="choreography" extend="activity">
        <add as="shape" value="activity"/>
