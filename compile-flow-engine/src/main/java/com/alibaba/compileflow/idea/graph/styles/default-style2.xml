    </add>
    <add as="callChoreographyTask" extend="choreography">
        <add as="call" value="1"/>
    </add>
    <add as="callChoreography" extend="subChoreography">
        <add as="call" value="1"/>
    </add>
    <add as="conversation" extend="defaultVertex">
        <add as="shape" value="conversationNode"/>
        <add as="verticalLabelPosition" value="top"/>
        <add as="verticalAlign" value="bottom"/>
        <add as="fillColor" value="#ABC5FF"/>
    </add>
    <add as="subConversation" extend="conversation">
        <add as="image" value="/com/timout/bpm/images/conversation_marker.png"/>
    </add>
    <add as="callConversation" extend="conversation">
        <add as="call" value="1"/>
        <add as="strokeWidth" value="3"/>
    </add>
    <add as="callCollaboration" extend="subConversation">
        <add as="call" value="1"/>
        <add as="strokeWidth" value="3"/>
    </add>
    <add as="annotation" extend="defaultVertex">
        <add as="shape" value="annotation"/>
        <add as="align" value="left"/>
    </add>
    <add as="dataObject" extend="defaultVertex">
        <add as="shape" value="dataobject"/>
        <add as="verticalLabelPosition" value="bottom"/>
        <add as="verticalAlign" value="top"/>
    </add>
    <add as="group" extend="defaultVertex">
