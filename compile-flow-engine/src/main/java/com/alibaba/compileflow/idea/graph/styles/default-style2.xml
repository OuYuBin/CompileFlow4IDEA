    </add>
    <add as="startEvent" >
        <add as="shape" value="event"/>
        <add as="perimeter" value="ellipsePerimeter"/>
        <add as="verticalLabelPosition" value="bottom"/>
        <add as="fillColor" value="#f0f5ff"/>
        <add as="shadow" value="1"/>
    </add>
    <add as="startMultipleEvent" extend="startEvent">
    </add>
    <add as="startParallelMultipleEvent" extend="startEvent">
    </add>
    <add as="endEvent" extend="event">
